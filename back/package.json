{
  "name": "planotterpocback",
  "version": "1.0.0",
  "description": "Few page app to get itinerary information for monuments",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "start:development": "NODE_ENV=development ./node_modules/.bin/nodemon ./dist/index.js",
    "postinstall:production": "npm run build",
    "build": "./node_modules/.bin/tsc -d --project ./tsconfig.json",
    "test": "jest --config ./jest.config.js",
    "console": "ts-node --project ./tsconfig.json ./dist/src/scripts/repl.js",
    "static": "find ./src -name \"*.csv\" -type f -type f -exec cp --parents {} ./dist \\;",
    "ts-node": "./node_modules/.bin/ts-node --project ./tsconfig.json"
  },
  "engines": {
    "node": "10.12.0"
  },
  "author": "me@austinflores.com",
  "license": "ISC",
  "dependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/csv-parse": "^1.1.11",
    "@types/mongodb": "^3.1.12",
    "@types/mongoose": "^5.2.20",
    "@types/mongoose-unique-validator": "^1.0.1",
    "apicaballero": "https://github.com/unflores/APICaballero.git",
    "awesome-typescript-loader": "^5.2.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "cookie-parser": "^1.4.3",
    "csv-parse": "^3.1.3",
    "express": "^4.16.3",
    "method-override": "^3.0.0",
    "mongoose": "^5.3.8",
    "mongoose-unique-validator": "^2.0.2",
    "morgan": "^1.9.1",
    "package.json": "^2.0.1",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.2.2",
    "webpack": "^4.0.0-alpha.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/node": "^10.12.18",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "ts-jest": "^23.10.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-loader": "^3.6.0"
  }
}
