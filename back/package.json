{
  "name": "suarecipe",
  "version": "1.0.0",
  "description": "Few page app to get itinerary information for monuments",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "NODE_ENV=test npx nodemon --inspect ./dist/index.js | bunyan",
    "build": "npx tsc -d --project ./tsconfig.json",
    "build:watch": "yarn run build -w",
    "build:production": "NODE_ENV=production npx tsc -d --project ./tsconfig.prod.json",
    "postbuild": "npm run copyStatic",
    "copyStatic": "node copyStatic.js ./ src ./dist",
    "tslint": "npx tslint --project ./tsconfig.json --config ./tslint.json -t verbose ",
    "test": "NODE_ENV=test nodemon --exec \"mocha -r ts-node/register --file ./src/specs/setup.ts src/**/*.spec.ts\"",
    "test:watch": "yarn test --watch",
    "console:development": "NODE_OPTIONS=--experimental-repl-await NODE_ENV=test ts-node --project ./tsconfig.json ./dist/src/scripts/repl.js",
    "seed": "NODE_ENV=development ts-node --project ./tsconfig.json ./src/scripts/seed",
    "static": "find ./src -name \"*.csv\" -type f -type f -exec cp --parents {} ./dist \\;"
  },
  "engines": {
    "node": "14.15.3"
  },
  "author": "me@austinflores.com",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "@types/algoliasearch": "^3.30.5",
    "@types/bluebird": "^3.5.24",
    "@types/csv-parse": "^1.1.11",
    "@types/dotenv": "^6.1.0",
    "@types/mongodb": "^3.6.3",
    "@types/mongoose": "^5.10.3",
    "algoliasearch": "^3.32.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bunyan": "^1.8.12",
    "cookie-parser": "^1.4.3",
    "csv-parse": "^3.1.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.3",
    "express-async-errors": "^3.1.1",
    "express-basic-auth": "^1.2.0",
    "method-override": "^3.0.0",
    "mongoose": "^5.10.3",
    "morgan": "^1.9.1",
    "package.json": "^2.0.1",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.5.1",
    "webpack": "^4.0.0-alpha.3"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.5",
    "@types/chai": "^4.2.11",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.3",
    "@types/hapi__joi": "^16.0.12",
    "@types/joi": "^14.3.4",
    "@types/method-override": "^0.0.31",
    "@types/mocha": "^7.0.2",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.14.0",
    "@types/sinon": "^9.0.8",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "mocha": "^7.1.0",
    "mongodb-memory-server-global": "^6.3.3",
    "nodemon": "^1.18.9",
    "sinon": "^9.2.1",
    "supertest": "^4.0.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-loader": "^3.6.0",
    "watch": "^1.0.2"
  }
}
